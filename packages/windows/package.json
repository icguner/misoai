{
  "name": "misoai-windows",
  "version": "1.0.0",
  "description": "Windows desktop automation library for misoAI",
  "keywords": [
    "Windows UI automation",
    "Windows AI testing",
    "Windows automation library",
    "Windows automation tool",
    "desktop automation"
  ],
  "main": "./dist/lib/index.js",
  "types": "./dist/types/index.d.ts",
  "files": ["bin", "dist", "examples", "README.md", "CHANGELOG.md"],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "dev": "modern dev",
    "build": "modern build -c ./modern.config.ts",
    "build:watch": "modern build -w -c ./modern.config.ts",
    "test": "vitest --run",
    "test:u": "vitest --run -u",
    "test:ai": "MIDSCENE_CACHE=true npm run test",
    "example:basic": "ts-node examples/basic-usage.ts",
    "example:ai": "ts-node examples/ai-automation.ts",
    "example:screenshot": "ts-node examples/screenshot-capture.ts",
    "example:window-control": "ts-node examples/window-manipulation.ts"
  },
  "dependencies": {
    "misoai-core": "1.0.9",
    "misoai-shared": "1.0.3",
    "misoai-web": "1.0.2",
    "@nut-tree/nut-js": "^4.2.0",
    "robotjs": "^0.6.0",
    "node-ffi-napi": "^2.5.0",
    "ref-napi": "^3.0.3",
    "ref-struct-di": "^1.1.1",
    "screenshot-desktop": "^1.15.0",
    "active-win": "^8.0.0",
    "windows-window-manager": "^1.0.0"
  },
  "devDependencies": {
    "@modern-js/module-tools": "2.60.6",
    "@types/node": "^18.0.0",
    "@types/robotjs": "^0.6.0",
    "dotenv": "16.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vitest": "3.0.5"
  },
  "keywords": [
    "windows",
    "desktop",
    "automation",
    "testing",
    "ai",
    "misoai",
    "ui-automation",
    "screenshot"
  ],
  "license": "MIT",
  "os": ["win32"],
  "cpu": ["x64", "ia32"],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
